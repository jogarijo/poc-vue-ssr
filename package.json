{
  "version": "0.0.1",
  "description": "",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "start": "run-p start:*",
    "start:apostrophe": "ADDRESS=localhost PORT=3000 node dist/app.js",
    "migrate": "node dist/app.js apostrophe-migrations:migrate",
    "dev": "run-p dev:*",
    "dev:apostrophe": "parcel watch src/app.js src/**/index.js src/config/*.js src/lib/modules/*/vue/*.vue src/lib/modules/*/public/*/*.{scss,js} --target node",
    "dev:assets": "cpx 'src/**/*.{html,png,jpg}' dist --watch",
    "serve": "ADDRESS=localhost PORT=3000 nodemon",
    "clean": "rimraf dist/*.js dist",
    "purge": "rimraf dist/*.js dist .cache",
    "rebuild": "run-s purge build"
  },
  "author": "Jonathan Garijo <jogarijo@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "apostrophe": "^2.89.1",
    "config": "^3.1.0",
    "fs-extra": "^8.0.1",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "cpx": "^1.5.0",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "sass": "^1.20.1",
    "vue-template-compiler": "^2.6.10"
  },
  "nodemonConfig": {
    "watch": [
      "dist"
    ],
    "ext": "js",
    "delay": 100
  }
}
